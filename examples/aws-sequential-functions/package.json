{
  "name": "aws-sequential-functions",
  "version": "1.0.0",
  "description": "AWS example using sequential functions",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "copy-libs": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./copy-my-library.ps1",
    "build": "npm run copy-libs && npm run lint-and-fix && tsc",
    "package": "npm run build && copy .\\package.json .\\dist",
    "sam-build-old": "npm run package && sam build",
    "sam-build": "npm run package && sam build -t templateGenerated.yaml",
    "delete-stack": "aws cloudformation delete-stack --stack-name aws-sequential-functions --region eu-west-2",
    "test": "mocha --require ts-node/register test/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/aws-lambda": "^8.10.48",
    "@types/deep-equal": "^1.0.1",
    "@types/node": "^13.11.1",
    "@types/uuid": "^7.0.2",
    "deep-equal": "^2.0.3",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.133.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.814.0",
    "aws-sdk-mock": "^5.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^10.2.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.9.5",
    "yaml": "^1.10.0"
  },
  "optionalDependencies": {}
}
